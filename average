def dictator(dict):
    something = {}
    for key,value in dict.items():
        average = float(sum(value)) / len(value)
        something[key] = average
    return something


print(dictator({"hi": [1, 3, 4, 6, 5, 2, 7], "no": [2, 4, 3, 6, 7, 8], "yes": [10, 34, 21, 34, 16], "done": [10, 7, 3, 4]}))
