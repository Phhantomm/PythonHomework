def average(dict):
        smth = ""
        count = 0
        for key,value in dict.items():
            if len(value) > count:
                smth = key
                count = len(value)
        return smth
print(average({"hi": [1, 3, 4, 6, 5, 2, 7], "no": [2, 4, 3, 6, 7, 8], "yes": [10, 34, 21, 34, 16], "done": [10, 7, 3, 4]}))
